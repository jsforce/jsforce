{
  "author": "Shinichi Tomita <shinichi.tomita@gmail.com>",
  "name": "@mixmaxhq/jsforce",
  "description": "Salesforce API Library for JavaScript",
  "keywords": [
    "salesforce",
    "salesforce.com",
    "sfdc",
    "force.com",
    "database.com"
  ],
  "homepage": "http://github.com/mixmaxhq/jsforce",
  "version": "1.8.3",
  "repository": {
    "type": "git",
    "url": "git://github.com/mixmaxhq/jsforce.git"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/mixmaxhq/jsforce/raw/master/LICENSE"
    }
  ],
  "main": "./index",
  "scripts": {
    "build": "gulp build",
    "build:test": "gulp build:test",
    "build:all": "gulp build:all",
    "doc": "jsdoc lib -d doc --recurse --lenient",
    "test": "npm run test:node",
    "test:all": "npm run test:node && npm run test:browser",
    "test:node": "mocha --retries 2 --reporter spec --require intelli-espower-loader",
    "test:browser": "testem",
    "prepublish": "node -e \"if(process.env.npm_package_version!==require('./lib/VERSION')){console.error('The pacakge.json version is not matching to ./lib/VERSION.js');process.exit(1)}\""
  },
  "directories": {
    "test": "test/"
  },
  "files": [
    "README.md",
    "bower.json",
    "LICENSE",
    "package.json",
    "bin",
    "build",
    "lib",
    "core.js",
    "index.js"
  ],
  "engines": {
    "node": ">=4.0"
  },
  "bin": {
    "jsforce": "./bin/jsforce"
  },
  "dependencies": {
    "co-prompt": "^1.0.0",
    "coffeescript": "^1.10.0",
    "commander": "^2.9.0",
    "csv-parse": "^2.5.0",
    "csv-stringify": "^1.0.4",
    "faye": "^1.2.0",
    "inherits": "^2.0.1",
    "lodash": "^4.11.1",
    "multistream": "^2.0.5",
    "open": "^7.0.0",
    "promise": "^7.1.1",
    "readable-stream": "^2.1.0",
    "request": "^2.72.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "archiver": "^1.0.0",
    "async": "^1.5.2",
    "browserify": "^16.5.0",
    "envify": "^3.4.0",
    "espower-loader": "^1.0.0",
    "espowerify": "^1.0.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^1.2.0",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "power-assert": "^1.3.0",
    "puppeteer": "^1.1.1",
    "require-swapper": "^0.1.7",
    "testem": "^1.7.1",
    "through2": "^2.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "browser": {
    "./index": "./lib/browser/jsforce",
    "./index.js": "./lib/browser/jsforce",
    "./core": "./lib/browser/core",
    "./core.js": "./lib/browser/core",
    "request": "./lib/browser/request.js"
  }
}
