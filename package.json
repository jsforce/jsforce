{
  "author": "Shinichi Tomita <shinichi.tomita@gmail.com>",
  "name": "jsforce",
  "description": "Salesforce API Library for JavaScript",
  "keywords": [
    "salesforce",
    "salesforce.com",
    "sfdc",
    "force.com",
    "database.com"
  ],
  "homepage": "http://github.com/jsforce/jsforce",
  "version": "2.0.0-alpha",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsforce/jsforce.git"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/jsforce/jsforce/raw/master/LICENSE"
    }
  ],
  "main": "./index",
  "module": "./module/index",
  "types": "./lib/index",
  "scripts": {
    "clean": "rimraf lib/* module/*",
    "build:types": "run-p build:types:es5 build:types:module",
    "build:types:es5": "npm run build:types:src -- --outDir lib",
    "build:types:module": "npm run build:types:src -- --outDir module",
    "build:types:src": "tsc --emitDeclarationOnly --declaration -p src",
    "build": "run-p build:types build:es5 build:module",
    "build:es5": "NODE_ENV=production npm run build:src -- -d lib",
    "build:module": "NODE_ENV=module npm run build:src -- -d module",
    "build:src": "babel src --extensions \".ts\"",
    "lint": "eslint --ext .ts,.js src test",
    "typecheck": "tsc --noEmit",
    "test": "jest `if (test \"${SF_POOL_USERNAME}\" = \"\"); then echo \"-w 1\"; else echo \"-w 5\"; fi`"
  },
  "directories": {
    "test": "test/"
  },
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "api",
    "bin",
    "build",
    "core",
    "lib",
    "test",
    "index.d.ts",
    "index.js"
  ],
  "engines": {
    "node": ">=8.0"
  },
  "bin": {
    "jsforce": "./bin/jsforce",
    "jsforce-gen-schema": "./bin/jsforce-gen-schema"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.7.6",
    "base64url": "^3.0.1",
    "commander": "^4.0.1",
    "csv-parse": "^4.8.2",
    "csv-stringify": "^5.3.4",
    "faye": "^1.2.4",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "multistream": "^4.0.0",
    "open": "^7.0.0",
    "regenerator-runtime": "^0.13.3",
    "request": "^2.88.0",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@types/fs-extra": "^8.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@types/puppeteer": "^2.0.0",
    "@types/request": "^2.48.3",
    "@types/through2": "^2.0.34",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-preset-power-assert": "^3.0.0",
    "conditional-type-checks": "^1.0.5",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0",
    "through2": "^3.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "browser": {
    "./lib/registry": "./lib/browser/registry.js",
    "./lib/registry/index.js": "./lib/browser/registry.js",
    "./module/registry": "./module/browser/registry.js",
    "./module/registry/index.js": "./module/browser/registry.js",
    "request": "./lib/browser/request.js"
  }
}
